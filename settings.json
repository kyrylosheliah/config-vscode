{
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[javascript]": {
    "editor.indentSize": 2
  },
  "[javascriptreact]": {
    "editor.indentSize": 2
  },
  "[json]": {
    "editor.indentSize": 2
  },
  "[jsx-tags]": {
    "editor.indentSize": 2
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[typescript]": {
    "editor.indentSize": 2
  },
  "auto-rename-tag.activationOnLanguage": [
    "*"
  ],
  "breadcrumbs.enabled": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "never",
    "source.organizeImports": "never"
  },
  //"editor.cursorSurroundingLines": 5,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.folding": false,
  "editor.formatOnSave": false,
  "editor.glyphMargin": true,
  "editor.guides.indentation": true,
  //"editor.hover.delay": 700,
  "editor.hover.enabled": true,
  "editor.largeFileOptimizations": true,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  //"editor.quickSuggestionsDelay": 1000,
  "editor.rulers": [
    {
      "color": "#3f3f3f",
      "column": 80
    },
    {
      "color": "#3f3f3f",
      "column": 120
    }
  ],
  "editor.stickyScroll.enabled": false,
  "editor.suggest.insertMode": "insert",
  "emmet.includeLanguages": {
    "django-html": "html",
    "javascript": "javascriptreact",
    "plaintext": "pug",
    "razor": "html",
    "typescript": "typescriptreact",
    "vue": "html",
    "vue-html": "html"
  },
  "emmet.preferences": {
    "files.associations": {
      "*html": "html"
    }
  },
  "explorer.confirmDelete": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.eol": "\n",
  "files.exclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.useEditorConfig": false,
  "print.alternateBrowser": false,
  "print.colourScheme": "Visual Studio",
  "print.lineNumbers": "off",
  "print.renderMarkdown": true,
  "python.analysis.extraPaths": [
    "D:/programs/anaconda3/Lib/site-packages/"
  ],
  "telemetry.telemetryLevel": "off",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "unocss.autocomplete.maxItems": 10,
  "update.showReleaseNotes": false,
  "window.commandCenter": false,
  "window.menuBarVisibility": "compact",
  //"window.restoreWindows": "none",
  "window.title": "${dirty}${separator}${activeEditorLong}${separator}${dirty}",
  "window.titleSeparator": " ",
  "workbench.activityBar.iconClickBehavior": "focus",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "[Default High Contrast]": {
      //"editor.background": "#00000030", // BREAKS SELECTION
      "contrastBorder": "#4f4f4f",
      "editorIndentGuide.activeBackground1": "#4f4f4f",
      "editorIndentGuide.activeBackground2": "#4f4f4f",
      "editorIndentGuide.activeBackground3": "#4f4f4f",
      "editorIndentGuide.activeBackground4": "#4f4f4f",
      "editorIndentGuide.activeBackground5": "#4f4f4f",
      "editorIndentGuide.activeBackground6": "#4f4f4f",
      "editorIndentGuide.background1": "#4f4f4f",
      "editorIndentGuide.background2": "#4f4f4f",
      "editorIndentGuide.background3": "#4f4f4f",
      "editorIndentGuide.background4": "#4f4f4f",
      "editorIndentGuide.background5": "#4f4f4f",
      "editorIndentGuide.background6": "#4f4f4f",
      "editorUnnecessaryCode.border": "#4f4f4f",
      "editorWhitespace.foreground": "#4f4f4f",
      "tree.inactiveIndentGuidesStroke": "#4f4f4f",
      "tree.indentGuidesStroke": "#4f4f4f"
    }
  },
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.showTabs": "none",
  "workbench.iconTheme": null,
  "workbench.layoutControl.enabled": false,
  "workbench.layoutControl.type": "menu",
  "workbench.sideBar.location": "right",
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "always",
  "zenMode.centerLayout": true,
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  //"zenMode.hideStatusBar": true // unset in order to user-toggle
  "zenMode.restore": false,
  "zenMode.showTabs": "none",
  "files.autoSave": "afterDelay"
}
{
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[javascript]": {
    "editor.indentSize": 2
  },
  "[javascriptreact]": {
    "editor.indentSize": 2
  },
  "[json]": {
    "editor.indentSize": 2
  },
  "[jsx-tags]": {
    "editor.indentSize": 2
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[typescript]": {
    "editor.indentSize": 2
  },
  "auto-rename-tag.activationOnLanguage": [
    "*"
  ],
  "editor.bracketPairColorization.enabled": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "never",
    "source.organizeImports": "never"
  },
  //"editor.cursorSurroundingLines": 5,
  //"editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.defaultFormatter": null,
  "editor.folding": false,
  "editor.formatOnSave": false,
  "editor.glyphMargin": true,
  "editor.guides.indentation": true,
  //"editor.hover.delay": 700,
  "editor.renderWhitespace": "all",
  "editor.hover.enabled": true,
  "editor.largeFileOptimizations": true,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  //"editor.quickSuggestionsDelay": 1000,
  "editor.rulers": [
    {
      "color": "#3f3f3f",
      "column": 80
    },
    {
      "color": "#3f3f3f",
      "column": 120
    }
  ],
  //"editor.cursorSurroundingLines": 200,
  "editor.stickyScroll.enabled": false,
  "editor.suggest.insertMode": "insert",
  "emmet.includeLanguages": {
    "django-html": "html",
    "javascript": "javascriptreact",
    "plaintext": "pug",
    "razor": "html",
    "typescript": "typescriptreact",
    "vue": "html",
    "vue-html": "html"
  },
  "emmet.preferences": {
    "files.associations": {
      "*html": "html"
    }
  },
  "explorer.confirmDelete": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.eol": "\n",
  "files.exclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.useEditorConfig": false,
  "print.alternateBrowser": false,
  "print.colourScheme": "Visual Studio",
  "print.lineNumbers": "off",
  "print.renderMarkdown": true,
  "telemetry.telemetryLevel": "off",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "unocss.autocomplete.maxItems": 10,
  "update.showReleaseNotes": false,
  "window.commandCenter": false,
  //"window.restoreWindows": "none",
  "window.title": "${dirty}${separator}${activeEditorLong}${separator}${dirty}",
  "window.titleSeparator": " ",
  "workbench.activityBar.iconClickBehavior": "focus",
  "workbench.colorCustomizations": {
    "editor.foreground": "#ffef7f",
    "contrastBorder": "#666666",
    "editorBracketMatch.background": "#660066",
    "editorBracketMatch.border": "#ff00ff", // ffbbff
    "editorIndentGuide.activeBackground1": "#666666",
    "editorIndentGuide.activeBackground2": "#666666",
    "editorIndentGuide.activeBackground3": "#666666",
    "editorIndentGuide.activeBackground4": "#666666",
    "editorIndentGuide.activeBackground5": "#666666",
    "editorIndentGuide.activeBackground6": "#666666",
    "editorIndentGuide.background1": "#666666",
    "editorIndentGuide.background2": "#666666",
    "editorIndentGuide.background3": "#666666",
    "editorIndentGuide.background4": "#666666",
    "editorIndentGuide.background5": "#666666",
    "editorIndentGuide.background6": "#666666",
    "editorUnnecessaryCode.border": "#666666",
    "editorWhitespace.foreground": "#666666",
    "tree.inactiveIndentGuidesStroke": "#666666",
    "tree.indentGuidesStroke": "#666666",
    // reduce inlay hint's contrast
    "editorInlayHint.background": "#333333",
    "editorInlayHint.foreground": "#cccccc",
    "editorInlayHint.parameterBackground": "#333333",
    "editorInlayHint.parameterForeground": "#cccccc",
    "editorInlayHint.typeBackground": "#333333",
    "editorInlayHint.typeForeground": "#cccccc",
  },
  "editor.semanticHighlighting.enabled": false,
  "editor.semanticTokenColorCustomizations": {
    "enabled": false
  },
  "editor.tokenColorCustomizations": {
    "comments": "#7f7f7f",
    "functions": "#ffffff",
    "keywords": "#ff8f8f",
    "numbers": "#ffbfff",
    "strings": "#8fff8f",
    "types": "#7fbfff",
    "variables": "#ffffff",
    "textMateRules": [
      {
        "scope": [
          "keyword.operator",
          "keyword.operator.logical.python"
        ],
        "settings": {
          "foreground": "#ffef7f",
        }
      },
      {
        "scope": "comment",
        "settings": {
          "foreground": "#7f7f7f"
        }
      },
      {
        "scope": [
          "variable",
          "entity.name.variable",
          "meta.definition.variable.name",
          "support.variable",
          // ...
          "meta.embedded",
          "source.groovy.embedded",
          "string meta.image.inline.markdown",
          "variable.legacy.builtin.python",
		      // ...
		      "meta.template.expression",
          // ...
          "entity.name.tag.css",
          "entity.name.tag.less",
          // ...
          "entity.other.attribute-name",
          // ...
          "entity.other.attribute-name.class.css",
          "entity.other.attribute-name.id.css",
          "entity.other.attribute-name.parent-selector.css",
          "entity.other.attribute-name.parent.less",
          "entity.other.attribute-name.pseudo-element.css",
          "entity.other.attribute-name.scss",
          "source.css variable",
          "source.css entity.other.attribute-name.class",
          "source.css entity.other.attribute-name.pseudo-class",
          "source.css.less entity.other.attribute-name.id",
          // ...
          "support.type.vendored.property-name",
          "support.type.property-name",
          "source.coffee.embedded",
          // ...
          "entity.name.function",
          "support.function",
          "support.constant.handlebars",
          "source.powershell variable.other.member"
        ],
        "settings": {
          "foreground": "#ffffff",
        }
      },
      {
        "scope": [
          "keyword",
          "storage.type",
          "storage.modifier",
          "support.function.builtin",
          // ...
          "keyword.operator.cast",
          "keyword.operator.sizeof",
          "keyword.operator.new",
          "keyword.operator.expression",
          // ...
          "variable.language.this",
          // ...
          "keyword.statement",
          // ...
          "keyword.control",
          "source.cpp keyword.operator.new",
          "source.cpp keyword.operator.delete",
          "keyword.other.using",
          "keyword.other.directive.using",
          "keyword.other.operator",
        ],
        "settings": {
          "foreground": "#ff8f8f",
        }
      },
      {
        "scope": [
          "keyword.constant",
          "constant.language",
          "constant.character",
          "constant.numeric",
        ],
        "settings": {
          "foreground": "#7fbfff",
        }
      },
      {
        "scope": [
          "meta.preprocessor.string",
          "string",
          "string.tag",
          "string.value",
          // ...
        ],
        "settings": {
          "foreground": "#7fff7f"
        }
      },
      {
        "scope": [
          "keyword.type",
          "constant.other.color.rgb-value",
          "constant.other.rgb-value",
          "support.constant.color",
          // ...
          "entity.name.tag",
          // ...
          "support.class",
          "support.type",
          "entity.name.type",
          "entity.name.namespace",
          "entity.name.scope-resolution",
          "entity.name.class",
          "storage.type.cs",
          "storage.type.generic.cs",
          "storage.type.modifier.cs",
          "storage.type.variable.cs",
          "storage.type.annotation.java",
          "storage.type.generic.java",
          "storage.type.java",
          "storage.type.object.array.java",
          "storage.type.primitive.array.java",
          "storage.type.primitive.java",
          "storage.type.token.java",
          "storage.type.groovy",
          "storage.type.annotation.groovy",
          "storage.type.parameters.groovy",
          "storage.type.generic.groovy",
          "storage.type.object.array.groovy",
          "storage.type.primitive.array.groovy",
          "storage.type.primitive.groovy"
        ],
        "settings": {
          "foreground": "#ffbfff"
        }
      },
      {
        "scope": [
          "emphasis",
          // ...
          "markup.italic"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "strong",
          // ...
          "markup.bold"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "meta.diff.header",
        "settings": {
          "foreground": "#000080"
        }
      },
      {
        "scope": "constant.regexp",
        "settings": {
          "foreground": "#B46695"
        }
      },
      {
        "scope": "invalid",
        "settings": {
          "foreground": "#F44747"
        }
      },
      {
        "scope": "markup.underline",
        "settings": {
          "fontStyle": "underline"
        }
      },
      {
        "scope": "markup.heading",
        "settings": {
          "foreground": "#6796E6",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.strikethrough",
        "settings": {
          "fontStyle": "strikethrough"
        }
      },
      {
        "scope": "markup.inserted",
        "settings": {
          "foreground": "#B5CEA8"
        }
      },
      {
        "scope": "markup.deleted",
        "settings": {
          "foreground": "#CE9178"
        }
      },
      {
        "scope": "markup.changed",
        "settings": {
          "foreground": "#569CD6"
        }
      },
      {
        "scope": [
          "punctuation.definition.tag"
        ],
        "settings": {
          "foreground": "#808080"
        }
      },
      {
        "scope": "meta.preprocessor",
        "settings": {
          "foreground": "#569CD6"
        }
      },
      {
        "scope": "meta.preprocessor.numeric",
        "settings": {
          "foreground": "#B5CEA8"
        }
      },
      {
        "scope": "meta.structure.dictionary.key.python",
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": "string.regexp",
        "settings": {
          "foreground": "#D16969"
        }
      },
      {
        "scope": [
          "punctuation.definition.template-expression.begin",
          "punctuation.definition.template-expression.end",
          "punctuation.section.embedded"
        ],
        "settings": {
          "foreground": "#569CD6"
        }
      },
      {
        "scope": "keyword.other.unit",
        "settings": {
          "foreground": "#B5CEA8"
        }
      },
      {
        "scope": "support.function.git-rebase",
        "settings": {
          "foreground": "#D4D4D4"
        }
      },
      {
        "scope": "constant.sha.git-rebase",
        "settings": {
          "foreground": "#B5CEA8"
        }
      },
      {
        "scope": [
          "storage.modifier.import.java",
          "variable.language.wildcard.java",
          "storage.modifier.package.java"
        ],
        "settings": {
          "foreground": "#D4D4D4"
        }
      },
      {
        "scope": [
          "meta.type.cast.expr",
          "meta.type.new.expr",
          "support.constant.math",
          "support.constant.dom",
          "support.constant.json",
          "entity.other.inherited-class"
        ],
        "settings": {
          "foreground": "#4EC9B0"
        }
      },
      {
        "scope": [
          "meta.object-literal.key"
        ],
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": [
          "support.constant.property-value",
          "support.constant.font-name",
          "support.constant.media-type",
          "support.constant.media",
          "constant.other.color.rgb-value",
          "constant.other.rgb-value",
          "support.constant.color"
        ],
        "settings": {
          "foreground": "#CE9178"
        }
      },
      {
        "scope": "meta.resultLinePrefix.contextLinePrefix.search",
        "settings": {
          "foreground": "#CBEDCB"
        }
      },
      {
        "scope": "token.info-token",
        "settings": {
          "foreground": "#6796E6"
        }
      },
      {
        "scope": "token.warn-token",
        "settings": {
          "foreground": "#008000"
        }
      },
      {
        "scope": "token.error-token",
        "settings": {
          "foreground": "#FF0000"
        }
      },
      {
        "scope": "token.debug-token",
        "settings": {
          "foreground": "#B267E6"
        }
      }
    ]
  },
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.showTabs": "none",
  "workbench.iconTheme": null,
  "workbench.layoutControl.enabled": false,
  //"workbench.layoutControl.type": "menu",
  "workbench.sideBar.location": "right",
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "always",
  "zenMode.centerLayout": true,
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  //"zenMode.hideStatusBar": true // unset in order to user-toggle
  "zenMode.restore": false,
  "zenMode.showTabs": "none",
  "files.autoSave": "off",
  "workbench.colorTheme": "Default High Contrast",
  "zig.zls.enabled": "on",
"zig.formattingProvider": "off",
"errorLens.enabledDiagnosticLevels": [
  "error",
  "warning",
  "info"
],
"breadcrumbs.enabled": false,
"dotnet.completion.showCompletionItemsFromUnimportedNamespaces": false,
"editor.codeActionWidget.includeNearbyQuickFixes": false,
"editor.acceptSuggestionOnCommitCharacter": false,
"window.menuBarVisibility": "compact",
"workbench.statusBar.visible": false,
"workbench.activityBar.location": "hidden",
}